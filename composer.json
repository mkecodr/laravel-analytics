{
    "name": "mkecodr/laravel-analytics",
    "description": "GA4 integration for laravel",
    "keywords": [
        "MkEcodr",
        "laravel",
        "laravel-analytics-v4"
    ],
    "homepage": "https://github.com/mkecodr/laravel-analytics",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "google/analytics-data": "^0.8.6",
        "illuminate/contracts": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.22.2|^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
   "autoload": {
        "psr-4": {
            "MkEcodr\\LaravelAnalyticsV4\\": "src",
            "MkEcodr\\LaravelAnalyticsV4\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MkEcodr\\LaravelAnalyticsV4\\Tests\\": "tests"
        }
    },
    "authors": [
        
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MkEcodr\\LaravelAnalyticsV4\\LaravelAnalyticsV4ServiceProvider"
            ],
            "aliases": {
                "LaravelAnalyticsV4": "MkEcodr\\LaravelAnalyticsV4\\Facades\\LaravelAnalyticsV4"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
